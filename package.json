{
  "name": "vuttr",
  "version": "1.1.0",
  "main": "server.js",
  "author": "Deivid Rodrigues <deivid.arodrigues@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/main/server.js",
    "build": "rimraf dist && tsc",
    "dev": "ts-node-dev --transpile-only --respawn --ignore-watch node_modules -r dotenv/config ./src/main/server.ts",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand --setupFiles dotenv/config",
    "test:verbose": "jest --passWithNoTests --runInBand --setupFiles dotenv/config",
    "test:unit": "yarn test --watch -c jest-unit.config.js",
    "test:integration": "yarn test --watch -c jest-integration.config.js",
    "test:staged": "yarn test --findRelatedTests",
    "test:ci": "yarn test --coverage",
    "typeorm": "ts-node node_modules/typeorm/cli.js"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/faker": "^4.1.12",
    "@types/jest": "^26.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.13",
    "@types/supertest": "^2.0.10",
    "@types/yup": "^0.29.3",
    "@typescript-eslint/eslint-plugin": "3.3.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard-with-typescript": "^18.0.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "faker": "^4.1.0",
    "git-commit-msg-linter": "2.4.4",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "10.2.11",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-node-dev": "^1.0.0-pre.51",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.25",
    "yup": "^0.29.1"
  },
  "engines": {
    "node": "14.x"
  }
}
